[nvidia-open-gpu]
lib-modules-path = "kernel/drivers/extra/video/nvidia/open-gpu"
objects-source = "__NVIDIA_MODULES__"

[nvidia-open-gpu.object-files."nvidia.o"]
link-objects = ["nv-interface.o", "nv-kernel.o"]

[nvidia-open-gpu.kernel-modules."nvidia.ko"]
link-objects = ["nvidia.o", "nvidia.mod.o"]

[nvidia-open-gpu.object-files."nvidia-modeset.o"]
link-objects = ["nv-modeset-interface.o", "nv-modeset-kernel.o"]

[nvidia-open-gpu.kernel-modules."nvidia-modeset.ko"]
link-objects = ["nvidia-modeset.o", "nvidia-modeset.mod.o"]

[nvidia-open-gpu.kernel-modules."nvidia-uvm.ko"]
link-objects = ["nvidia-uvm.o", "nvidia-uvm.mod.o"]
